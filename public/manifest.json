{
  "manifest_version": 3,
  "name": "Size Compare",
  "version": "1.0",
  "description": "Compare clothing measurements to your own items.",
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.grailed.com/listings/*"],
      "js": ["content.js"]
    }
  ]
}
